{
  "name": "clawtrade-bnb",
  "version": "1.1.0",
  "displayName": "ClawTrade BNB - Autonomous DeFi Trading Agent",
  "description": "One-click autonomous yield farming agent with explainability panel, multi-agent UI, and real on-chain execution on BNB Chain.",
  "category": "defi",
  "difficulty": "advanced",
  "author": "Open Web Academy",
  "license": "MIT",
  "repository": "https://github.com/open-web-academy/clawtrade-bnb",
  "keywords": [
    "trading",
    "yield-farming",
    "autonomous-agent",
    "defi",
    "bnb-chain",
    "explainability",
    "multi-agent",
    "reinforced-learning"
  ],
  "requirements": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "commands": {
    "start": {
      "description": "Start agent scheduler + dashboard UI",
      "usage": "clawtrade start [--port 3001] [--network testnet|mainnet]"
    },
    "agent": {
      "description": "Control agent (start|pause|stop)",
      "usage": "clawtrade agent <action>"
    },
    "dashboard": {
      "description": "Start dashboard UI only",
      "usage": "clawtrade dashboard [--port 5173]"
    },
    "status": {
      "description": "Print runtime health + last cycle",
      "usage": "clawtrade status"
    },
    "logs": {
      "description": "Tail recent actions",
      "usage": "clawtrade logs [--limit 20] [--filter HARVEST|COMPOUND|REBALANCE]"
    },
    "demo": {
      "description": "Show demo checklist + URLs",
      "usage": "clawtrade demo"
    },
    "network": {
      "description": "Switch network (testnet|mainnet)",
      "usage": "clawtrade network <testnet|mainnet>"
    },
    "metrics": {
      "description": "Show performance metrics",
      "usage": "clawtrade metrics [--json]"
    }
  },
  "ports": {
    "agent": {
      "port": 3001,
      "description": "Agent API + log server",
      "configurable": true
    },
    "ui": {
      "port": 5173,
      "description": "Dashboard frontend",
      "configurable": true
    }
  },
  "environment": {
    "PRIVATE_KEY": {
      "required": true,
      "description": "Wallet private key (testnet)"
    },
    "RPC_URL": {
      "required": false,
      "default": "https://bsc-testnet.publicnode.com",
      "description": "BNB Testnet RPC endpoint"
    },
    "NETWORK": {
      "required": false,
      "default": "testnet",
      "options": ["testnet", "mainnet"],
      "description": "Target network"
    },
    "AGENT_PORT": {
      "required": false,
      "default": "3001",
      "description": "API server port"
    },
    "UI_PORT": {
      "required": false,
      "default": "5173",
      "description": "Dashboard port"
    },
    "OPERATOR_MODE": {
      "required": false,
      "default": "auto_execute",
      "options": ["auto_execute", "suggest_only"],
      "description": "Agent mode"
    },
    "RISK_PROFILE": {
      "required": false,
      "default": "balanced",
      "options": ["conservative", "balanced", "aggressive"],
      "description": "Risk tolerance"
    }
  },
  "features": [
    "One-Click AI Yield Operator - Set it and forget it",
    "Explainability Panel - See WHY every decision was made",
    "Multi-Agent Visual UI - Monitor agent team in real-time",
    "Real On-Chain Execution - Verified transactions on BNB Testnet/Mainnet",
    "Reinforced Learning - Auto-improves over time",
    "Network Switching - Testnet â†” Mainnet one command",
    "Append-Only Audit Trail - Every action logged and verifiable"
  ],
  "skills": {
    "core": {
      "strategy-engine": "Executes 3 autonomous trading strategies",
      "scheduler": "Cycles every 60 seconds with safety checks",
      "learning": "Reinforced learning optimizer"
    },
    "ui": {
      "dashboard": "Professional operator UI + explainability",
      "agent-team": "Multi-agent activity monitor",
      "metrics": "Real-time performance analytics"
    },
    "blockchain": {
      "execution": "Real transactions on BNB Chain",
      "logging": "On-chain audit trail",
      "verification": "BscScan link generation"
    }
  },
  "quickstart": {
    "step1": "npm install",
    "step2": "cp .env.example .env && nano .env (add PRIVATE_KEY)",
    "step3": "npm run start",
    "step4": "open http://localhost:3001 (or http://localhost:5173)"
  },
  "supportedNetworks": [
    {
      "name": "BNB Testnet",
      "chainId": 97,
      "rpc": "https://bsc-testnet.publicnode.com",
      "scanner": "https://testnet.bscscan.com"
    },
    {
      "name": "BNB Mainnet",
      "chainId": 56,
      "rpc": "https://bsc-dataseed1.defibit.io",
      "scanner": "https://bscscan.com"
    }
  ]
}
