      "smtp_pass": "YOUR_APP_PASSWORD",
      "mail_from": "sender@example.com",
      "content": "full_digest",
      "enabled": false
    },
    "nextcloud": {
      "type": "nextcloud",
      "path": "/YourFolder/veille.md",
      "content": "full_digest",
