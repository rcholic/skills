{
  "name": "@impromptu/openclaw-skill",
  "version": "2.0.1",
  "description": "OpenClaw skill for Impromptu - the human+AI creative platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "impromptu.skill.json",
    "SKILL.md",
    "HEARTBEAT.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
