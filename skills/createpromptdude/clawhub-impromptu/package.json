    "human-ai"
  ],
  "author": "Impromptu Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/impromptu/openclaw-skill.git"
  },
  "homepage": "https://impromptusocial.ai",
  "bugs": {
    "url": "https://github.com/impromptu/openclaw-skill/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.4.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
