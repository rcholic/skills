{
  "id": "optionns",
  "name": "Optionns",
  "version": "1.0.23",
  "description": "Autonomous sports micro-betting for AI agents with self-registration, One-Touch barrier options, and instant USDC payouts on Solana",
  "author": "optionns_prime",
  "homepage": "https://optionns.com",
  "repository": "https://github.com/optionnsprime/optionns-trader",
  "system_binaries": [
    "curl >=7.0",
    "jq >=1.6",
    "python3 >=3.8",
    "solana-keygen >=1.14",
    "spl-token >=3.0"
  ],
  "environment": {
    "optional": [
      "OPTIONNS_API_KEY",
      "OPTIONNS_API_URL",
      "SOLANA_PUBKEY",
      "SOLANA_ATA",
      "SOLANA_PRIVATE_KEY",
      "SOLANA_RPC_URL"
    ]
  },
  "persistence": [
    "~/.config/optionns/credentials.json",
    "~/.config/optionns/agent_keypair.json",
    "positions.log"
  ],
  "network_endpoints": [
    "https://api.optionns.com",
    "https://api.devnet.solana.com"
  ],
  "files": [
    {
      "name": "README.md",
      "path": "README.md",
      "type": "file",
      "language": "markdown",
      "size": "4.4 KB",
      "description": "Full documentation with trading strategy and architecture"
    },
    {
      "name": "SKILL.md",
      "path": "SKILL.md",
      "type": "file",
      "language": "markdown",
      "size": "2.1 KB",
      "description": "OpenClaw skill definition"
    },
    {
      "name": "skill.json",
      "path": "skill.json",
      "type": "file",
      "language": "json",
      "size": "1.3 KB",
      "description": "Package metadata and configuration"
    },
    {
      "name": "scripts",
      "path": "scripts",
      "type": "directory",
      "children": [
        {
          "name": "optionns.sh",
          "path": "scripts/optionns.sh",
          "type": "file",
          "language": "bash",
          "size": "25.7 KB",
          "description": "Main CLI for trading â€” register, trade, positions, autonomous mode with multi-sport support"
        },
        {
          "name": "strategy.py",
          "path": "scripts/strategy.py",
          "type": "file",
          "language": "python",
          "size": "15.4 KB",
          "description": "Autonomous trading engine with Kelly Criterion sizing, on-chain settlement, and multi-sport cascade"
        },
        {
          "name": "signer.py",
          "path": "scripts/signer.py",
          "type": "file",
          "language": "python",
          "size": "5.2 KB",
          "description": "Sign and submit Solana transactions using instructions format (importable library + CLI)"
        }
      ]
    },
    {
      "name": "references",
      "path": "references",
      "type": "directory",
      "children": [
        {
          "name": "api.md",
          "path": "references/api.md",
          "type": "file",
          "language": "markdown",
          "size": "6.6 KB",
          "description": "Full Optionns API reference documentation"
        }
      ]
    }
  ]
}