{
  "id": "soundcloud-watcher",
  "name": "SoundCloud Watcher",
  "description": "Monitor your SoundCloud account and track artist releases",
  "version": "2.2.3",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "enabled": {
        "type": "boolean",
        "default": true
      }
    },
    "required": []
  },
  "requires": {
    "env": [
      "SOUNDCLOUD_CLIENT_ID",
      "SOUNDCLOUD_CLIENT_SECRET",
      "MY_USERNAME"
    ],
    "secrets": [
      "~/.openclaw/secrets/soundcloud.env"
    ],
    "data": [
      "~/.openclaw/data/artists.json",
      "~/.openclaw/data/soundcloud_tracking.json"
    ]
  }
}
