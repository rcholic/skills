{
  "plugins": {
    "enabled": true,
    "entries": {
      "soundcloud-watcher": {
        "enabled": true,
        "config": {
          "clientId": "YOUR_SOUNDCLOUD_CLIENT_ID",
          "clientSecret": "YOUR_SOUNDCLOUD_CLIENT_SECRET",
          "username": "your_soundcloud_username",
          "checkIntervalHours": 6,
          "myTracksLimit": 10,
          "dormantDays": 90,
          "sessionKey": "agent:main:main"
        }
      }
    }
  }
}
